<div id="availableJourneyModal" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">

            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Trajet disponible</h4>
            </div>

            <div class="modal-body">

                <div class="form-group">
                    <div class="row">
                        <div class="col-md-12">

                            <form id="editAvailableJourney" action="#" method="post">
                                <input id="availableJourneyId" name="availableJourneyId" type="hidden">

                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <label for="voyageName">Depuis</label>
                                        </div>
                                        <div class="col-md-8">
                                            <span id="fromDestinationModalName"></span>
                                            <input id="fromDestinationModalId" name="fromDestinationId" type="hidden">
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <label for="voyageName">Jusqu'à</label>
                                        </div>
                                        <div class="col-md-8">
                                            <span id="toDestinationModalName"></span>
                                            <input id="toDestinationModalId" name="toDestinationId" type="hidden">
                                        </div>
                                        <div class="col-md-8 col-ms-offset-4">
                                            <p id="latAndLon"></p>
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <label for="flyPrices">Avion</label>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="input-group">
                                                <input id="flyPrices" class="form-control" name="flyPrices" placeholder="prix" type="number">
                                                <span class="input-group-addon">€</span>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="input-group">
                                                <input id="flyTime" class="form-control" name="flyTime" placeholder="temps" type="number">
                                                <span class="input-group-addon">min</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <label for="trainPrices">Train</label>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="input-group">
                                                <input id="trainPrices" class="form-control" name="trainPrices" placeholder="prix" type="number">
                                                <span class="input-group-addon">€</span>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="input-group">
                                                <input id="trainTime" class="form-control" name="trainTime" placeholder="temps" type="number">
                                                <span class="input-group-addon">min</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <label for="busPrices">Bus</label>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="input-group">
                                                <input id="busPrices" class="form-control" name="busPrices" placeholder="prix" type="number">
                                                <span class="input-group-addon">€</span>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="input-group">
                                                <input id="busTime" class="form-control" name="busTime" placeholder="temps" type="number">
                                                <span class="input-group-addon">min</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="form-group">
                                    <div class="row">
                                        <button type="submit" id="_submit" class="btn btn-raised btn-primary btn-block" name="_submit"
                                        data-loading-text="Sauvegarde en cours...">
                                        Enregistrer
                                        </button>
                                    </div>
                                </div>

                            </form>

                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div>