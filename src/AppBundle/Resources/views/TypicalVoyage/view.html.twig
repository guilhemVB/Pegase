{% set title = "Idées de voyages" %}
{% set titleMetaData = "Les meilleurs idées de voyage." %}
{% set keywords = "idée, de voyage, tour du monde, tour d'europe, tour d'asie, tour d'amérique, backpack" %}
{% set description = "Les meilleurs idées de voyage pour les backpackeurs. Tour du monde, tour d'Europe, tour d'Asie ou tour d'Amérique, tout y est !" %}
{% set type = "article" %}
{% set imageUrl = image_banner_homepage_path ~ 'temple_raw.JPG' %}

{% set activeIdeasOfTravels = true %}
{% extends "::layout.html.twig" %}


{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('/css/bootstrap-social.css') }}">
{% endblock %}


{% block body %}
    <div id="voyagesView">


        <div class="container-fluid containerOrange">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="page-header">
                            <h1>Idées de voyages</h1>
                        </div>
                    </div>
                </div>
            </div>
        </div>


        <div class="container-fluid">
            <div class="container">
                <div class="row">
                    {% for typicalVoyage in typicalsVoyage %}
                        {% include 'AppBundle:TypicalVoyage:typicalVoyage.html.twig' %}
                    {% endfor %}
                </div>
            </div>
        </div>

        {% include 'AppBundle:TypicalVoyage:typicalVoyageShare.html.twig' %}

    </div>
{% endblock %}
