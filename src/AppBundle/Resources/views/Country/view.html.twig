{% extends "::layout.html.twig" %}

{% block title %}
{{ country.name }} - {{ parent() }}
{% endblock %}

{% block body %}
<div class="container-fluid">
    {{ country.name }}
    {{ country.description }}
    <div id="gmap" style="width: 900px;height: 300px;"></div>
    <div id="controls"></div>
</div>
{% endblock %}


{% block javascripts %}
    <script src="http://maps.google.com/maps/api/js?sensor=false&libraries=geometry&v=3.7"></script>
    {{ parent() }}
    <script src="{{ asset('/js/maplace.js') }}"></script>
    <script src="{{ asset('bundles/app/js/country/maplaceCountry.js') }}"></script>

{% endblock %}


